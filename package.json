{
  "name": "uber-turbo",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev ",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "proto:gen": "protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=packages/proto/generated --ts_proto_opt=outputServices=grpc-js  --proto_path=packages/proto  packages/proto/*.proto"
  },
  "devDependencies": {
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.7.15",
    "@turbo/gen": "^2.5.4",
    "prettier": "^3.6.2",
    "protoc-gen-ts": "^0.8.7",
    "ts-proto": "^2.7.5",
    "turbo": "^2.5.4",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.8.2",
  "workspaces": [
    "apps/*",
    "packages/*",
    "services/*",
    "gateway"
  ],
  "dependencies": {
    "schema-dts": "^1.1.5"
  }
}