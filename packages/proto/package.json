{
  "name": "@repo/proto",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "generate": "protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=src/generated --ts_proto_opt=outputServices=grpc-js --proto_path=src/__protoschema src/__protoschema/*.proto && tsx  ./src/scripts/update-export.ts",
    "google:gen": "protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=src/generated --ts_proto_opt=outputServices=grpc-js --proto_path=./node_modules/google-proto-files ./node_modules/google-proto-files/google/maps/routing/v2/routes_service.proto && tsx  ./src/scripts/update-export.ts",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "build": "tsc -p tsconfig.json"
  },
  "devDependencies": {
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.7.15",
    "@repo/typescript-config": "workspace:*",
    "ts-proto": "^2.7.5",
    "tsx": "^4.7.1"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.9.0"
  },
  "exports": {
    ".": {
      "types": "./src/generated/index.d.ts",
      "import": "./src/generated/index.js",
      "default": "./src/generated/index.js"
    },
    "./auth": {
      "types": "./src/generated/auth.d.ts",
      "import": "./src/generated/auth.js",
      "default": "./src/generated/auth.js"
    },
    "./place": {
      "types": "./dist/generated/place.d.ts",
      "import": "./dist/generated/place.js",
      "default": "./dist/generated/place.js"
    },
    "./route": {
      "types": "./src/generated/route.d.ts",
      "import": "./src/generated/route.js",
      "default": "./src/generated/route.js"
    },
    "./routes": "./dist/generated/route.js"
  }
}
